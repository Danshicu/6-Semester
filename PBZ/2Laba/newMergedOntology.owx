        <AnnotationProperty abbreviatedIRI="rdfs:label"/>
        <IRI>http://purl.org/net/VideoGameOntology#purchasesGameOffering</IRI>
        <Literal xml:lang="en">purchases game offering</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
