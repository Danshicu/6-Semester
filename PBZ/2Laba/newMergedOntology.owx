        <AnnotationProperty abbreviatedIRI="rdfs:label"/>
        <IRI>http://purl.org/net/VideoGameOntology#isFriendWithPlayer</IRI>
        <Literal xml:lang="en">is friend with player</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>http://purl.org/net/VideoGameOntology#isItemInGame</IRI>
        <Literal xml:lang="en">The vgo:isItemInGame is used to specify which item belongs to a particular game. An item cannot exist alone and thus should always be associated to a certain game.</Literal>
    </AnnotationAssertion>
